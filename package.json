{
  "name": "storyteller",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yubrot/storyteller.git"
  },
  "author": "yubrot",
  "license": "BSD-3-Clause",
  "scripts": {
    "dev": "npm run dev:graphql & npm run dev:next",
    "dev:graphql": "graphql-codegen --watch",
    "dev:next": "next dev",
    "build": "npm run build:graphql && npm run build:next",
    "build:graphql": "graphql-codegen",
    "build:next": "next build && next export",
    "test": "vitest run",
    "ladle": "ladle serve",
    "lint": "next lint"
  },
  "dependencies": {
    "d3-scale-chromatic": "^3.0.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-generic-sdk": "^2.3.10",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@ladle/react": "^0.15.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "autoprefixer": "^10.4.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4",
    "vitest": "^0.12.9"
  }
}
